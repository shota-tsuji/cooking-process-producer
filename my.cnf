# Set character-set
# https://dev.mysql.com/doc/refman/8.0/en/option-files.html

[client]
default-character-set=utf8mb4

[mysql]
default-character-set=utf8mb4

[mysqldump]
default-character-set=utf8mb4

# https://dev.mysql.com/doc/refman/8.0/en/charset-server.html
[mysqld]
character-set-server=utf8mb4
